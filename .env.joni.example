# JONI Auto-Configuration Example
# Copy this file to .env.joni and fill in your API keys

# Main API Keys
ANTHROPIC_API_KEY=sk-ant-YOUR_KEY_HERE
GEMINI_API_KEY=YOUR_GEMINI_KEY_HERE
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_KEY_HERE

# Skill-specific API Keys
# These will be picked up by the skill configuration
JONI_SKILL_NANO_BANANA_PRO_API_KEY=YOUR_GEMINI_KEY_HERE
JONI_SKILL_OPENAI_WHISPER_API_API_KEY=YOUR_OPENAI_KEY_HERE

# Skip Google Places (not needed)
JONI_SKILL_GOPLACES_SKIP=true
JONI_SKILL_LOCAL_PLACES_SKIP=true
JONI_SKILL_NOTION_SKIP=true
JONI_SKILL_OPENAI_IMAGE_GEN_SKIP=true
